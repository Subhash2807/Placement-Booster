            <div class="box1">
                <div class="boxes">
                    <a href="/home/take-test?subject=dbms"><h2 class="blue">DBMS</h2></a>
                    
                </div>
            </div>



            <div class="box1">
                <div class="boxes">
                    <a href="/home/take-test?subject=os"><h2 class="blue">OS</h2></a>
                    
                </div>
            </div>



            <div class="box1">
                <div class="boxes">
                    <a href="/home/take-test?subject=ds"><h2 class="blue">Data Structure</h2></a>
                </div>
            </div>

        </div>


        <div class="intro">
            <H2 class="two">Flash Card</H2> <br>




            <div class="box1">
                <div class="boxes">
                    
                    <a href=""><h2 class="blue">OOPs</h2></a>
                </div>
            </div>


            <div class="box1">
                <div class="boxes">
                    <a href=""><h2 class="blue">DBMS</h2></a>
                    
                </div>
            </div>



            <div class="box1">
                <div class="boxes">
                    <a href=""><h2 class="blue">OS</h2></a>
                    
                </div>
            </div>



            <div class="box1">
                <div class="boxes">
                    <a href=""><h2 class="blue">Data Structure</h2></a>
                </div>
            </div>

        </div>


        <div class="intro">
            <H2 class="two">Notes</H2> <br>




            <div class="box1">
                <div class="boxes">
                    
                    <a href=""><h2 class="blue">OOPs</h2></a>
                </div>
            </div>


            <div class="box1">
                <div class="boxes">
                    <a href=""><h2 class="blue">DBMS</h2></a>
                    
                </div>
            </div>



            <div class="box1">
                <div class="boxes">
                    <a href=""><h2 class="blue">OS</h2></a>
                    
                </div>
            </div>



            <div class="box1">
                <div class="boxes">
                    <a href=""><h2 class="blue">Data Structure</h2></a>
                </div>
            </div>

        </div>


