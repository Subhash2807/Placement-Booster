            <h2>Previous test scores</h2>
            <table>
                <tr>
                    <th>S.No</th>
                    <th>Subject</th>
                    <th>Score</th>
                </tr>
                
                    <% user.scores.forEach((score,ind)=>{ %>
                        <tr>
                            <td><%=ind+1%></td>
                            <td><%=score.subject%></td>
                            <td><%=score.marks%></td>
                        </tr>
                    <% }) %> 
                
            </table>
        </div>
        


       



    </div>
    
